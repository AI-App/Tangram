rm -rf libtangram/
install -D ../../x86_64-unknown-linux-musl/libtangram.a libtangram/x86_64-unknown-linux-musl/libtangram.a
install -D ../../aarch64-unknown-linux-musl/libtangram.a libtangram/aarch64-unknown-linux-musl/libtangram.a
install -D ../../x86_64-apple-darwin/libtangram.a libtangram/x86_64-apple-darwin/libtangram.a
install -D ../../aarch64-apple-darwin/libtangram.a libtangram/aarch64-apple-darwin/libtangram.a
install -D ../../x86_64-pc-windows-gnu/libtangram.a libtangram/x86_64-pc-windows-gnu/libtangram.a
if [ ! -e dist ]; then
	git clone git@github.com:tangramxyz/tangram-go dist
fi
rsync --archive --delete --exclude .git --exclude .gitignore --exclude dist --exclude docs --exclude scripts . dist
git -C dist add --all
git -C dist commit
git -C dist push
